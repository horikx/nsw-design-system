<div class="nsw-section{{#if backgroundColor}} bg-gwic-sky-blue{{else}} theme--dark{{/if}} nsw-section--no-padding">
  <div class="nsw-container ">
    <h1 class="nsw-section-title">{{title}}</h1>
  </div>
</div>
{{#if navitems.length}}
<div class="nsw-section{{#if backgroundColor}} bg-gwic-sky-blue{{else}} theme--dark{{/if}} nsw-section--no-padding mb-4">
  <div class="nsw-container ">
    <div class="gwic-sub-navigation{{#unless responsiveCollapse}} no-collapse{{/unless}}">
      <div class="gwic-sub-navigation__btn-ctr">
        <button type="button" class="gwic-sub-navigation-btn gwic-sub-navigation-btn--menu js-open-sub-navigation"
          aria-expanded="false" aria-controls="main-navigation">
          <i class="material-icons nsw-material-icons gwic-sub-navigation-btn__icon" focusable="false"
            aria-hidden="true">menu</i>
          <span class="gwic-sub-navigation-btn__sub"><span class="sr-only">Open Menu</span></span>
        </button>
        <button type="button" class="gwic-sub-navigation__close js-close-sub-navigation" aria-expanded="true">
          <i class="material-icons nsw-material-icons" focusable="false" aria-hidden="true">close</i>
          <span class="sr-only">Close Menu</span>
        </button>
      </div>

      <div class="gwic-sub-navigation__list-ctr">
        <ul class="gwic-sub-navigation__list">
          {{#each navitems}}
          <li class="gwic-navigation__list-item">
            <a href="{{url}}" class="gwic-navigation__link d-flex flex-column">
              {{#with img}}
              <span class="flex-grow-1">
                <span class="gwic-navigation__icon text-center">
                  {{{icon}}}
                </span>
                <span class="gwic-navigation__icon gwic-navigation__icon--hover text-center">
                  <div class="gwic-navigation--circle"></div>
                  {{{icon}}}
                </span>
              </span>
              {{/with}}
              <span class="gwic-navigation__link-text flex-shrink-1 ">
                {{text}}
              </span>
            </a>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
{{/if}}